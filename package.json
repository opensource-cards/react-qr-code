{
  "name": "react-qr-code",
  "version": "0.1.1",
  "description": "A <QRCode /> component for React.",
  "keywords": [
    "react",
    "qr",
    "code",
    "QRCode"
  ],
  "author": {
    "name": "Ross Khanas",
    "email": "rtkhanas@gmail.com"
  },
  "scripts": {
    "demo": "cd demo && yarn start",
    "demo:build": "cd demo && yarn run build",
    "demo:deploy": "gh-pages -d demo/build",
    "storybook-web": "cd ./storybooks/web && npm run storybook",
    "storybook-native-watch": "babel ./src --out-dir=./storybooks/native/lib --watch",
    "storybook-native": "cd ./storybooks/native && npm run storybook",
    "storybook-emulator-ios": "cd ./storybooks/native && react-native run-ios",
    "storybook-ios": "npm run storybook-native-watch & npm run storybook-native & npm run storybook-emulator-ios",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "build": "npm run lint && npm test && npm run clean && babel src --out-dir lib"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:rtkhanas/react-qr-code.git"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "qr.js": "0.0.0"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-art": "^0.15.1",
    "react-native": "^0.41.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-allow-top-level-this": "^0.0.1",
    "babel-preset-react": "^6.23.0",
    "chai": "^4.1.0",
    "enzyme": "^2.6.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "gh-pages": "^1.0.0",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.1.0",
    "react": "~15.6.1",
    "react-addons-test-utils": "~15.6.0",
    "react-art": "^0.15.1",
    "react-dom": "~15.6.1",
    "react-native": "^0.46.1",
    "rimraf": "^2.5.3"
  }
}
